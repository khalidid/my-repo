pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world!' 
            }
        }
        stage('Checkout external proj') {
        steps {
            git branch: 'master',
                credentialsId: 'khalidid',
                url: 'ssh://git@github.com/khalidid/my-repo.git'

            sh "ls -lat"
        }
    }
        stage('Checkout code') {
           steps {
            checkout scm
        }
    }
    }
}